# past the nearest line that does not start with a space.
multiline_sort () {
    ${PERL} -e '
        use warnings;
        use strict;
        my @buffer = ();
        while (<STDIN>) {
            if (/^ /) {
                push(@buffer, $_);
            } else {
                print $_ foreach sort(@buffer);
                print $_;
                @buffer = ();
            }
        }
        print $_ foreach sort(@buffer);
'
}
m4_divert_pop([PREPARE_TESTS])

